{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-expressions": "warn",
        "no-console": "off",
        "func-names": "off",
        "class-methods-use-this": "off",
        "imort/no-dynamic-require": "off",
        "indent": 2
    },
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {}
}